<form action="" id="pdfExtractor">
    <h3>{{pdfUrl}}</h3>
    <div class="flexrow">
        sdfsqdfqsdfsqdfsqdfsqdfsqdfsqdf
        <div class="form-fields">
            {{filePicker target="pdfUrl" }}
            <input type="text" id="pdfUrl" name="pdfUrl" placeholder="{{localize
                'File Path'}}" value="{{pdfUrl}}" />
        </div>
        <!--
        <a id="scanPage">
            <i class="fab fa-searchengin"></i>scan
        </a>
        -->
        <a id="createFolders">
            <i class="fas fa-magic"></i>create folders<i class="fa-solid
                fa-folder-tree"></i>
        </a>


    </div>
    <div class="flexrow main">

        <iframe
            id="pdfReader"
            src="modules/pdfReaderTest/pdfjs/web/viewer.html?file=../../../../{{pdfUrl}}"
            {{#unless pdfUrl}}
            style=" display:none"
            {{/unless}}></iframe>
        {{#unless pdfUrl}}
        <h2>choose a pdf file</h2>
        {{/unless}}
        <div>
            {{#each sizes as |size el|}}
            <input type="text" value="{{size}}"/>

            {{el}}

            {{/each}}

        </div>
    </div>


</form>